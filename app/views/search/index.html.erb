<div class="row">
  <div class="col-md-6 offset-md-3">
    <%= form_with url: "/search", method: :get, local:true do |form| %>
      <div class="field">
        <%#= form.label :query, "検索" %>
        <%= form.text_field :user_name %>
      </div>
      <div class="actions">
        <%= form.submit "検索", class: "btn btn-main-green", id: "post-btn"%>
    <% end %>
  </div>
</div>